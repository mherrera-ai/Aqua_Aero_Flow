{
  "name": "aqua-aero-flow",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Interactive particle flow visualization with audio-reactive modes",
  "author": "Aqua Aero Flow",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "format": "prettier --write \"src/**/*.{js,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "check:all": "npm run lint && npm run format:check && npm run test -- --run"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.55.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "happy-dom": "^18.0.1",
    "prettier": "^3.6.2",
    "terser": "^5.43.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "particles",
    "visualization",
    "interactive",
    "webgl",
    "canvas",
    "generative-art",
    "audio-reactive",
    "pwa"
  ]
}
